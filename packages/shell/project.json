{
  "name": "shell",
  "sourceRoot": "packages/shell/src",
  "projectType": "application",
  "targets": {
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "piral debug",
        "cwd": "packages/shell"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "piral build",
        "cwd": "packages/shell"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "piral validate",
        "cwd": "packages/shell"
      }
    }
  }
}
