{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "containerRegistryUrl": {
      "value": ""
    },
    "containerImageName": {
      "value": "augmentservice:latest"
    },
    "redisSku": {
      "value": "Basic"
    },
    "redisCapacity": {
      "value": 0
    },
    "containerPort": {
      "value": 8080
    },
    "containerCpus": {
      "value": "0.5"
    },
    "containerMemory": {
      "value": "1Gi"
    },
    "containerReplicas": {
      "value": 1
    },
    "daprHttpPort": {
      "value": 3500
    },
    "daprGrpcPort": {
      "value": 50001
    }
  }
}
