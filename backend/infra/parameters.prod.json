{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "mfe-portal-prod"
    },
    "location": {
      "value": "eastus"
    },
    "containerRegistryUrl": {
      "value": "${AZURE_CONTAINER_REGISTRY_ENDPOINT}"
    },
    "containerImageName": {
      "value": "augmentservice:latest"
    },
    "redisSku": {
      "value": "Standard"
    },
    "redisCapacity": {
      "value": 1
    },
    "containerPort": {
      "value": 8080
    },
    "containerCpus": {
      "value": "1"
    },
    "containerMemory": {
      "value": "2Gi"
    },
    "containerReplicas": {
      "value": 3
    },
    "daprHttpPort": {
      "value": 3500
    },
    "daprGrpcPort": {
      "value": 50001
    }
  }
}
